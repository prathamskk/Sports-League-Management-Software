        <PasswordField fx:id="edtpassword" layoutX="233.0" layoutY="200.0" />
        <TextField fx:id="edtusername" layoutX="233.0" layoutY="147.0" />
        <Label layoutX="168.0" layoutY="151.0" text="Username" />
        <Label layoutX="170.0" layoutY="204.0" text="Password" />
        <Button fx:id="btnlogin" layoutX="339.0" layoutY="247.0" mnemonicParsing="false" onAction="#CheckLoginPerformed" text="login" />
        <Button fx:id="btnregister" layoutX="233.0" layoutY="247.0" mnemonicParsing="false" onAction="#CheckLoginPerformed" text="register" />
    </children>
